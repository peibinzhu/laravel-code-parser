{
  "name": "peibin/laravel-code-parser",
  "description": "Peibin code-parser component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "laravel"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "peibin/laravel-support": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\CodeParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\CodeParser\\": "tests/"
    }
  },
  "suggest": {
    "jean85/pretty-package-versions": "Required to use PrettyVersions. (^1.2|^2.0)",
    "nikic/php-parser": "Required to use PhpParser. (^4.0)"
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
